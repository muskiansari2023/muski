CREATE TABLE Teacher_detail(
Teacher_id INT NOT NULL,
first_name VARCHAR(255) NOT NULL,
Last_name VARCHAR(255) NOT NULL,
Email VARCHAR(255) NOT NULL,
Address VARCHAR(255) DEFAULT 'THANE',
Salary INT NOT NULL,
primary key(Teacher_id));

